<div local-class="dashboard_row_header">
  <div local-class="dashboard_row_header_title_top">
    {{@projectTitleTop}}
  </div>
  {{#if @projectTitle}}
  <div local-class="dashboard_row_header_title">
    <br/>
    <hr/>
    <br/>
    {{@projectTitle}}
  </div>
  {{/if}}
  <div local-class="dashboard_row_header_description">
    {{@projectDescription}}
  </div>
</div>
<div local-class="dashboard_row_container">
  <div local-class="dashboard_row">
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_image">
        <picture>
          <source srcset={{@blockOneImage}} loading="lazy" alt="Project" type="image/webp" height="135" width="135">
          <img src={{@blockOneImageFallBack}} loading="lazy" alt="Project" height="280" width="280"> 
        </picture> 
      </div>
    </div>
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_icon_container">
        {{#if @iconOne}}
        <div local-class="dashboard_block_icon">
          <picture>
          <source srcset={{@iconOne}} loading="lazy" alt="Project" type="image/webp" height="152" width="136">
          <img src={{@iconOneFallBack}} loading="lazy" alt="Project" height="152" width="136"> 
        </picture>
        </div>
        {{/if}}
        {{#if @iconTwo}}
        <div local-class="dashboard_block_icon">
          <img src="{{@iconTwo}}" loading="lazy" alt="icon" height="241" width="133">
        </div>
        {{/if}}
        {{#if @iconThree}}
        <div local-class="dashboard_block_icon">
          <img src="{{@iconThree}}" loading="lazy" alt="icon" height="241" width="133">
        </div>
        {{/if}}
      </div>
      <div local-class="dashboard_block_label">
        {{@blockTwoLabel}}
      </div>
    </div>
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_text">
        {{@blockThreeTextOne}} <br />
        {{@blockThreeTextTwo}} <br />
        {{@blockThreeTextThree}} <br />
        {{@blockThreeTextFour}} <br />
        {{@blockThreeTextFive}} <br />
      </div>
      <div local-class="dashboard_block_label">
        {{@blockThreeLabel}}
      </div>
    </div>
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_text">
        {{@blockFourTextOne}} <br />
        {{@blockFourTextTwo}} <br />
        {{@blockFourTextThree}} <br />
        {{@blockFourTextFour}} <br />
        {{@blockFourTextFive}} <br />
      </div>
      <div local-class="dashboard_block_label">
        {{@blockFourLabel}}
      </div>
    </div>
    <a href="{{@projectGithubUrl}}" target="_blank" rel="noopener noreferrer" local-class="nav_link first">
      <div local-class="dashboard_block">
        <div local-class="dashboard_block_icon_container">
          <div local-class="dashboard_block_icon">
            <picture>
              <source srcset="https://roderickdevelopers.s3.us-east-2.amazonaws.com/gitlogo.webp" loading="lazy" type="image/webp" height="132" width="241">
              <img src="https://roderickdevelopers.s3.us-east-2.amazonaws.com/gitlogo.png" loading="lazy" alt="Code" height="132" width="241">
            </picture>
          </div>
        </div>
        <div local-class="dashboard_block_label">
          {{@blockFiveLabel}}
        </div>
      </div>
    </a>
    {{#if @projectLiveUrl}} 
    <a href="{{@projectLiveUrl}}" target="_blank" rel="noopener noreferrer">
      <div local-class="dashboard_block">
        <div local-class="dashboard_block_icon_container">
          <div local-class="dashboard_block_text">
            <picture>
              <source srcset="https://roderickdevelopers.s3.us-east-2.amazonaws.com/livesite.webp" loading="lazy" type="image/webp" height="133" width="241">
              <img src="https://roderickdevelopers.s3.us-east-2.amazonaws.com/livesite.png" loading="lazy" alt="Live Site" height="133" width="241">
            </picture>
          </div>
        </div>
        <div local-class="dashboard_block_label">
          {{@blockSixLabel}}
        </div>
      </div>
    </a>
    {{/if}}
  </div>
</div>

