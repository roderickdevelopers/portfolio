<div local-class="contact_component_container">
  <div local-class="contact_content_container">
    <div local-class="contact_content_container_row top">
      <h1>Let's Connect</h1>
      <p>{{this.paragraphOne}}</p>
    </div>
    <div local-class="contact_card">
      <picture>
        <source srcset="https://roderickdevelopers.s3.us-east-2.amazonaws.com/contact.webp" loading="lazy" type="image/webp" height="149" width="241">
        <img src="https://roderickdevelopers.s3.us-east-2.amazonaws.com/contact.png" loading="lazy" alt="Contact Icon" height="149" width="241">
      </picture>
      <div local-class="contact_title">
        <form action="https://formsubmit.co/roderickdevelopers@gmail.com" method="POST">
          <div local-class="contact_form">
            <label>
              <p>Full Name</p>
              <input type="text" name="name" placeholder="Your Name" required>
            </label>
          </div>
          <div local-class="contact_email">
            <label>
              <p>Email</p>
              <input type="email"
                pattern="^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$"
                name="email" placeholder="your@email.com" required>
            </label>
          </div>
          <div local-class="text_area">
            <label>
              <p>Message</p>
              <textarea name="message" placeholder="Your Message" required></textarea>
            </label>
          </div>
          <div local-class="contact_send">
            <button type="submit"> Send
              <input type="hidden" name="_captcha" value="false">
              <input type="text" name="_honey" style="display:none">
              <input type="hidden" name="_next" value="https://roderickdevelopers.netlify.app/thank-you">
              <input type="hidden" name="_template" value="table">
            </button>
          </div>
          <div local-class="some_other_section">
          <a href="https://www.linkedin.com/in/roderickdevelopers/" aria-label="Linkedin">
            <FaIcon @icon="linkedin" @prefix="fab" @size=2x /></a>
          <a href="https://github.com/roderickdevelopers/" aria-label="GitHub">
            <FaIcon @icon="github-square" @prefix="fab" @size=2x /></a>
          <a href="https://twitter.com/Roderickdev" aria-label="Twitter">
            <FaIcon @icon="twitter-square" @prefix="fab" @size=2x /></a>
        </div>
        </form>
      </div>
    </div>
  </div>
</div>