<div local-class="blog_component_container">
  <div local-class="blog_content_container">
    <div local-class="blog_content_container_row top">
      <h1>Blog Posts</h1>
    </div>
    <div local-class="blog_content_container_row">
      <div local-class="blog_flipcard_container">
        <LinkTo @route="blog.ember-mobile-menu">
          <Dynamic::Ui::Card @imageFront={{this.imageFrontCardOne}} loading="lazy" @imageFrontFallBack={{this.imageFrontFallBack}} loading="lazy" />
        </LinkTo>
        <div local-class="button_container">
          <LinkTo @route="blog.ember-mobile-menu">
            <button type="button" aria-label="Blog Post">Blog Post</button>
          </LinkTo>
        </div>
      </div>
      <div local-class="blog_flipcard_container">
        <LinkTo @route="blog.git-for-beginners">
          <Dynamic::Ui::Card @imageFront={{this.imageFrontCardTwo}} loading="lazy"
            @imageFrontFallBack={{this.imageFrontFallBackTwo}} loading="lazy" />
        </LinkTo>
        <div local-class="button_container">
          <LinkTo @route="blog.git-for-beginners">
            <button type="button" aria-label="Blog Post">Blog Post</button>
          </LinkTo>
        </div>
      </div>
    </div>
  </div>
</div>